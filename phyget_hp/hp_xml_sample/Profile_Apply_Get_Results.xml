<!--  Copyright 2012,2016 Hewlett Packard Enterprise Development LP  -->

<!-- Description:  This is a sample XML script to get the results    -->
<!--               from the Profile_Apply.                           -->

<!-- NOTE:  You will need to replace the USER_LOGIN and PASSWORD     -->
<!--        values with values that are appropriate for your         -->
<!--        environment.                                             -->

<!--        Use HPQLOCFG.EXE v1.00 or greater with this script.      -->

<!--        See "iLO Scripting and Command Line Guide" for more      -->
<!--        information on scripting and the syntax of the RIBCL     -->
<!--        XML.                                                     -->

<!--        Firmware support information for this script:            -->
<!--            iLO 5 - All versions                                 -->
<!--            iLO 4 - Version 1.10 or later                        -->

<RIBCL VERSION="2.0">
  <LOGIN USER_LOGIN="adminname" PASSWORD="password">
    <RIB_INFO MODE="read">
      <PROFILE_APPLY_GET_RESULTS/>
    </RIB_INFO>
  </LOGIN>
</RIBCL>